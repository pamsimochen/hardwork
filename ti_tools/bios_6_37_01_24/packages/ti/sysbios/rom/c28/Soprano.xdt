Void ti_sysbios_rom_c28_Soprano_romVersionCheck()
{
    UChar *byte = (UChar *)(0x3f8054);

    if (*byte == 0x216C) {
        /* This is the latest version of the ROM (from SYS/BIOS 6.37.01) */
    }
    else {
        /* This is the old version of the ROM. Spin forever */
        while (1);
    }
}
